modules = ["nodejs-20", "web", "python-3.11"]

[nix]
channel = "stable-24_05"

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 3003
externalPort = 80

[[ports]]
localPort = 34061
externalPort = 3003

[[ports]]
localPort = 37803
externalPort = 4200

[[ports]]
localPort = 42699
externalPort = 3002

[[ports]]
localPort = 46593
externalPort = 3001

[agent]
expertMode = true
integrations = ["github:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "GitHub Sync"

[[workflows.workflow]]
name = "GitHub Sync"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "echo \"Ready to sync to GitHub! Run 'npm run sync' to push your changes.\""

[workflows.workflow.metadata]
outputType = "console"
